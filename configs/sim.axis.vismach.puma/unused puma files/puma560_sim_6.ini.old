[EMC]
VERSION = 1.1
MACHINE = LinuxCNC-HAL-SIM-PUMA560
#+ Debug level, 0 means no messages. See src/emc/nml_int/emcglb.h for others
DEBUG = 0

[KINS]
JOINTS= 6
KINEMATICS= genserkins

[DISPLAY]
DISPLAY = axis
CYCLE_TIME = 0.200
HELP_FILE = tklinuxcnc.txt
POSITION_OFFSET = RELATIVE
POSITION_FEEDBACK = ACTUAL
DEFAULT_ANGULAR_VELOCITY = 200.0
MAX_FEED_OVERRIDE = 20.0
MAX_ANGULAR_VELOCITY = 200
PROGRAM_PREFIX = /home/freemoore/linuxcnc/nc_files
INTRO_GRAPHIC = linuxcnc.gif
INTRO_TIME = 5
PYVCP = puma.xml
#EDITOR = gedit

[TASK]
TASK = milltask
CYCLE_TIME = 0.010

[RS274NGC]
PARAMETER_FILE = puma.var

[EMCMOT]
EMCMOT = motmod
COMM_TIMEOUT = 1.0
SERVO_PERIOD = 1000000
TRAJ_PERIOD = 10000000

[HAL]
HALUI = halui
# andy added next line
# HALFILE = joypad_v3.hal


HALFILE = puma560_sim_6.hal
HALFILE = puma560_xbox360_postgui.hal

POSTGUI_HALFILE = puma560_postgui.hal

[TRAJ]
COORDINATES = X Y Z A B C
LINEAR_UNITS = mm
ANGULAR_UNITS = deg
DEFAULT_LINEAR_VELOCITY = 100.0
MAX_LINEAR_VELOCITY = 1000.0
DEFAULT_LINEAR_ACCELERATION = 10000.0
MAX_LINEAR_ACCELERATION = 20000.0

[EMCIO]
EMCIO =                  io
CYCLE_TIME = 0.100
TOOL_TABLE = puma.tbl

[AXIS_X]
MAX_VELOCITY = 300.0
MAX_ACCELERATION = 2000.0
MIN_LIMIT = -180.0
